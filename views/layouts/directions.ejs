                  <h3 class='trip-planner-header'>Plan Your Trip</h3>
                </div>

                <!-- split map view -->
                <div class='trip-planner-row' ng-controller="toFromCtrl">
                  <form>
                    <div class='trip-planner-from'>
                      <p class='label-small'>Where are you coming from?</p>
                      <div><span class='from-icon'></span><input id="from-text" type='text' ng-model="fromText"></input></div>
                    </div>
                    <div class='trip-planner-to'>
                      <p class='label-small'>Going too...</p>
                      <div>
                        <span class='from-icon'></span><input type='text'></input>
                        <input type='submit' value='GO' ng-click="updateDirections($event);"></input>
                      </div>
                    </div>
                  </form>
                  <div class='trip-planner-maps'>
                    <div class='trip-planner-from ggnpc-map-container'>
                      <div class='ggnpc-map' ggnpc-map map-height='400'></div>
                    </div>
                    <div class='trip-planner-to ggnpc-map-container'>
                      <div class='ggnpc-map' ggnpc-map map-height='400'></div>
                    </div>
                  </div>
                </div>

                <!-- menu-ish thing -->
                <div class='trip-planner-row'>
                  <div>That's <span class='trip-planner-distance'></span>, and should take <span class='trip-planner-time'></span> to get there
                    <select class='trip-planner-mode' ng-model='travelMode'>
                      <option>By transit</option>
                      <option>By car</option>
                      <option>By bike</option>
                      <option>By foot</option>
                    </select>
                </div>

